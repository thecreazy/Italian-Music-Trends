{
    "trackLyrics": {
        "syncType": "UNSYNCED",
        "lines": [
            {
                "startTimeMs": "0",
                "words": "P. Vallesi/B. Dati",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Firenze disegnata da bambini,",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Noi fatti d'aria in giro per le vie",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Il cuore in mano i libri ed i giornalini",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Tu non immagini i cambiamenti!",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Firenze appesa come un calendario",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Sulla parete della vanit?",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ognuno in tasca ha il suo vocabolario",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E il tempo sbriciola i monumenti.",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Il cielo di Firenze",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Nei tuoi occhi era sereno",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Oggi piove sofferenze",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E la vita ha un giorno in meno",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ormai",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Firenze vende e tutti in centro!",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E niente da sperare",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Soli? triste starci dentro",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E affondi prima o poi.",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Firenze insegue il desiderio",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "? Stupita sul serio!",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E di notte vuol peccare",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Per poi farsi perdonare",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Come noi.",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Firenze l'anima",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "La mia pelle!",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Firenze accesa come un lampadario",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Sotto il soffitto dell'eternit?",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Il suo silenzio? il sonno di un acquario",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Firenze polvere tra le stelle.",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Inutili violenze",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Stanno trasformando in ghetto",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Anche il cielo di Firenze",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Che da sempre? il nostro tetto",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ma",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Firenze sogna un'altra vita",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Questa? troppo amara",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Dove la gente? tutta amica.",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Vedrai ci riuscir?!",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Firenze che non vuol morire",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ci parla senza dire",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E io l'ascolto e te lo scrivo",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Qui da dove siamo nati",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Da quei due bambini disegnati .",
                "syllables": []
            }
        ],
