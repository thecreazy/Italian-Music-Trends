    "trackLyrics": {
        "syncType": "UNSYNCED",
        "lines": [
            {
                "startTimeMs": "0",
                "words": "Sick Luke, Sick Luke",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "♪",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E mi ritrovo qui a scriverti senza misure",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Senza pensarci, senza soldi in tasca né cure",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Che tutto perde senso se non ci sei al mio fianco",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "È un po' che non ti penso, ma non mi aiuta affatto",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E piove forte fuori dalla finestra",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Sono sola sotto 'sta coperta",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "È un giorno triste e camera è deserta",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Un po' ti ho maledetta, ma non rimpiango niente",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Manco l'ultima scelta (l'ultima scelta)",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "È stato un viaggio ed ora torno (ed ora torno)",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ma sai che in fondo voglio te",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E perdo un pezzo, ma non mi pento",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E resto senza un perché",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E tu non mi guardi più (no)",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Mentre io affondo (yeah)",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Poi cadiamo giù come acqua",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Nel giorno più triste del mondo (yeah)",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Era pomeriggio, lo ricordo bene",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Fuori era uno schifo, dentro stiamo bene",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Tu eri quasi in tiro, io vestito a caso",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Una maglia viola e le sneakers nere",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "C'era il sole alto",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Come un classico giorno che inizia normale e che invece nasconde dell'altro",
                "syllables": []
            },
            {
