{
    "trackLyrics": {
        "syncType": "UNSYNCED",
        "lines": [
            {
                "startTimeMs": "0",
                "words": "Sono con un amico mio",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "È la sera del 6 gennaio e siamo a S. Martino",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "È il mio ultimo giorno a Napoli",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Sono qui per le vacanze di natale",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Piove, ma non fa tanto freddo e non c'è troppo casino",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Un po' guardiamo la città",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E un po' ci guardiamo tra di noi",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Parlando di Lavezzi e della solitudine degli eroi",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E gli racconto",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Che da un po' di tempo",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Spesso mi viene in mente",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Quella poesia di Dante.",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Quella dove lui diceva",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Quanto mi piacerebbe",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Che il tempo si fermasse",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Metre io me ne sto su una barca",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Con un paio di amici miei e qualche amichetta",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "A parlare d'amore e a cazzeggiare",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Tipo i video dei rapper americani",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E poi gli dico",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Che non me li aspettavo",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Tutti questi addii",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Pensavo che ogni cosa che amavo",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Poi rimaneva mia",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Però pensavo male",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E il fatto è che non c'è",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Una regola o una morale",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Da ricavare",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Sono con lo stesso amico mio",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E' una sera di alcuni anni dopo, lo stringo vicino",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Siamo affacciati da una finestra qualunque, sopra un cortile qualunque",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ma questa notte ci sentiamo vicinissimi al nostro destino",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Parliamo del male",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "In generale, di quello che abbiamo ricevuto",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E di quello che comettiamo noi",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Lui mi dice",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "\"Lo senti il rumore dell'universo",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "In questo momento?",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ti posso dare un bacio sulla bocca?",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Lo sai che noi non moriremo mai?",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Io penso che no: che non lo so",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ma non glielo dico",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E poi gli do un bacio a questo mio amico",
