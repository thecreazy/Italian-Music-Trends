{
    "trackLyrics": {
        "syncType": "UNSYNCED",
        "lines": [
            {
                "startTimeMs": "0",
                "words": "Anche quando ci buttiamo via",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Per rabbia o per vigliaccheria",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Per un amore inconsolabile",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Anche quando in casa il posto è più invivibile",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E piangi e non lo sai che cosa vuoi",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Credi",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "C'è una forza in noi amore mio",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Più forte dello scintillio",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Di questo mondo pazzo e inutile",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "È più forte di una morte incomprensibile",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E di questa nostalgia",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Che non ci lascia mai",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Quando toccherai",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Il fondo con le dita",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "A un tratto sentirai",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "La forza della vita",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Che ti trascinerà",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Con se",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Amore non lo sai",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Vedrai una via d'uscita c'è",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Anche quando mangi per dolore",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E nel silenzio senti il cuore",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Come un rumore insopportabile",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E non vuoi più alzarti",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E il mondo è irraggiungibile",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E anche quando la speranza",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Oramai non basterà",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "C'è una volontà",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Che questa morte sfida",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "È la nostra dignità",
                "syllables": []
            },
            {
