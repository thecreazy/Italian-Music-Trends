{
    "trackLyrics": {
        "syncType": "UNSYNCED",
        "lines": [
            {
                "startTimeMs": "0",
                "words": "Nella notte, mentre tutti dormono",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Solo un uomo cammina per le strade",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "É un uomo in frack",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "É giunta mezzanotte, si spengono i rumori",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Si spegne anche l'insegna di quell'ultimo caffè",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Le strade son deserte",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Deserte e silenziose",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Un'ultima carrozza, cigolando, se ne va",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Il fiume scorre lento, frusciando sotto i ponti",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "La luna splende in cielo, dorme tutta la città",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Solo va un uomo in frack",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ha il cilindro per cappello, due diamanti per gemelli",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Un bastone di cristallo, la gardenia nell'occhiello",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E sul candido gilet un papillon",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Un papillon di seta blu",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "S'avvicina lentamente, con incedere elegante",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ha l'aspetto trasognato, malinconico ed assente",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Non si sa da dove viene, nè dove va",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Chi mai sarà quell'uomo in frack?",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Bonne nuit, bonne nuit, bonne nuit, bonne nuit",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Buonanotte",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Va dicendo ad ogni cosa, ai fanali illuminati",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ad un gatto innamorato che randagio se ne va",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "♪",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "É giunta ormai l'aurora, si spengono i fanali",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Si sveglia a poco a poco tutta quanta la città",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "La luna si é incantata, sorpresa e impallidita",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Pian piano scolorandosi nel cielo sparirà",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Sbadiglia una finestra sul fiume silenzioso",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E nella luce bianca galleggiando se ne va",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Un cilindro, un fiore e un frack",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Galleggiando dolcemente, lasciandosi cullare",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Se ne scende lentamente sotto i ponti, verso il mare",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Verso il mare se ne va, di chi sarà?",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Di chi sarà quel vecchio frack?",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Adieu, adieu, adieu, adieu",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Addio al mondo",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ai ricordi del passato",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ad un sogno mai sognato",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ad un attimo d'amore che mai più ritornerà",
                "syllables": []
            },
            {
