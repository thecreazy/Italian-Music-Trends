        "lines": [
            {
                "startTimeMs": "0",
                "words": "Un uomo dovrebbe esser' così grande",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Da capire quanto è piccolo",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Usare bene le parole, lame infuocate nel sole",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Cavalcare frasi fino all'imbrunire",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Fino all'alba dove un viaggio può arrivare",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Fermarsi solo per non scappare, prima di andare via",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Non bastano un'asse e una corda",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Per fare di te un impiccato",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ti han' detto sei sempre il migliore",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Eppure qualcuno ha sparato",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Se un uomo con una pistola",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Incontra chi tiene un fucile",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Il primo può solo pregare",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E prepararsi a morire",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E tutto correva veloce",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Aveva i suoi uomini accanto",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "A un tratto qualcosa cambiò quel sorriso in un pianto",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "La gente nemmeno credeva a ciò che vedeva",
                "syllables": []
            },
            {
