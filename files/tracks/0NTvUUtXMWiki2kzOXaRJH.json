        "lines": [
            {
                "startTimeMs": "0",
                "words": "Va bene va tutto bene sorge la torre di Babele",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Tra le macerie sopra le ceneri dell'Eden fanno un cantiere",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ma troppe voci fanno bla bla bla bla bla bla",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "C'era in principio una lingua bambina",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Adamo con il dito puntato diceva collina",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Nuvola donna una mora squisita",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ecco prendi questa nocciolina",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Guarda ho pescato un pesce facciamo il fuoco calore buono",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Parlare è un gioco giochiamo",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Se Eva e Adamo ad esempio dicevano vento",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "La parola sfiorava il flusso dell'aria",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E quasi ricreava il suo suono perfetto",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "L'indice puntava esattamente sull'oggetto",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "L'uomo che guardava era una parte del progetto",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ma quando disse mela",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Il serpente maledetto disse uomo prendila fanne il tuo strumento",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Quindi lo introdusse alla ragione del possesso",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E Adamo staccando la mela dal ramo staccava se stesso",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Dal tutto",
                "syllables": []
            },
            {
