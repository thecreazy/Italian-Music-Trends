        "lines": [
            {
                "startTimeMs": "0",
                "words": "Alla fine mi sono venduto il cuore",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Era pesante era vano era pieno di buchi",
                "syllables": []
            },
            {
