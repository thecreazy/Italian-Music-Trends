{
    "trackLyrics": {
        "syncType": "UNSYNCED",
        "lines": [
            {
                "startTimeMs": "0",
                "words": "That's Shune, a-am I right?",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Non fanno più canzoni come \"Guantanamera\"",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Che bella, io pensavo fosse \"Agguanta una mela\"",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Chissà quante non ne avrò capite",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Mi fa stare sempre appiccicato a terra",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Siamo grandi, ma non vuole dire solamente crescere",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Essere quello che avremmo voluto essere",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Sopra un trampolino che non si può flettere",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Schivo un avversario che non si può stendere",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E io che stupido, pensavo di essere informato bene",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ma per informarsi al meglio serve",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Una dose di rassegnazione",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Che fa da costituzione a chi la verità pretende",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E non avrei mai fatto un cazzo",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Se avessi ascoltato te, dopo te, pure te, pure tu",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ma ho le frequenze di un matto",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E non mi arriva il messaggio, è occupato (tu-tu)",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Andre cantava, cantava, cantava",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Senza lezioni, senza chitarra",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Senza la gente che lo guardava",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Fuori di casa senza la giacca",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Andre cantava, cantava e urlava",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Senza un tovagliolo per la bava",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Senza il solfeggio, senza l'ottava",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Senza nessuno che lo ascoltava",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Andre cantava, cantava, cantava",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Cantava, cantava sempre di più",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Senza sapere chi stesse ascoltando",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Senza sapere nulla di nulla",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E poi cantava, cantava, cantava",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Senza la fretta di andare su",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Con la valigia in mezzo alla piazza",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "In mezzo alla gente, ma senza il tour",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "♪",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Andre cantava, cantava, cantava",
