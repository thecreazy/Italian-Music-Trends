    "trackLyrics": {
        "syncType": "UNSYNCED",
        "lines": [
            {
                "startTimeMs": "0",
                "words": "Seh, seh, eh",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Quando piove verso sera questo cielo trema",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Puoi calmarti nella baia di queste mie braccia",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ogni parola nata in gola è come una balena",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Che si arena sulla sabbia di queste tue labbra",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E ogni giorno che vola, ogni giorno a ogni ora",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ti rivedo sola che incanti la pioggia",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Nella nostra bolla c'è il tuono e l'aurora",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Tu fumi e poi ti esce un fantasma di bocca",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Dentro ogni goccia che ora vola vedo un mondo intero, ehi",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E dentro mille versioni di noi",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Un multiverso dove il cielo vive prigioniero, ehi",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Dentro ai contorni di questi occhi tuoi",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Come specchi infranti e pianeti distanti",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Negli stessi istanti, ma in mondi diversi",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Fra atomi e quanti e i destini incrociati",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Qua moltiplicati per mille universi",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "E in uno tu taci, in un altro mi sfiori",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ad un tratto, ma noi non ci siamo mai visti",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "In un altro mi baci fra mille colori",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ed è un quadro, un paesaggio di pioggia, Kandinskij",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Con gli occhi sui bordi di ogni tuo pensiero",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Io vedo qui mille incredibili vie",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ogni volta che scegli è un mondo parallelo",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Che si apre fra brividi e mille vertigini",
                "syllables": []
            },
            {
                "startTimeMs": "0",
                "words": "Ogni goccia di pioggia che cade dal cielo traccia mille possibili scie",
                "syllables": []
            },
            {
